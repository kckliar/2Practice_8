#include <iostream>
#include <vector>
#include <cmath>



int wex()
{
    using namespace std;

    vector <int> number;
    string num;

    cout << "Число:\n";
    cin >> number;

    for(int i=0; i < number.size(); i++) number.push_back(num[i]);
    int dot;
    for(int j=0; j < number_soul.size(); j++){
        if(number_soul[j] == '.') dot = j;
    }

    double number_d = 0;
    for(int left = 1; left <= dot; left++){
        number_d+= (number_soul[left - 1] - 48) * pow(10,dot - left);
    }

    int right = 1;
    while (dot + right < number.size()){
        number_d+= (number)soul[dot + right] - 48) * pow(10,- right);
        right++;
    }

    cout << number_d;
    return 0;
}
